# Copy this file to .env and fill in your actual Supabase credentials

# Your Supabase project URL (found in Settings > API)
SUPABASE_URL=https://your-project-ref.supabase.co

# Your Supabase service role key (found in Settings > API)
# This is the secret service_role key, NOT the anon public key
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...your-service-role-key

# Your database password (set when you created your Supabase project)
SUPABASE_DB_PASSWORD=your-database-password

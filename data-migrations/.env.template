# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase project URL
SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key (found in Project Settings > API)
SUPABASE_ANON_KEY=your_anon_key_here

# Optional: Data file path (defaults to ../data/shots_2024.csv)
CSV_FILE_PATH=../data/shots_2024.csv

# Optional: Table name (defaults to nhl_shots_2024)
TABLE_NAME=nhl_shots_2024

# Optional: Batch size for uploads (defaults to 1000)
BATCH_SIZE=1000
